generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SpinResult {
  id        String   @id @default(uuid())
  sessionId String
  discount  Int
  createdAt DateTime @default(now())

  @@index([sessionId])
  @@index([createdAt])
}
