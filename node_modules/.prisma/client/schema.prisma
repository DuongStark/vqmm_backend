generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SpinResult {
  id        String   @id @default(uuid())
  sessionId String
  discount  Int
  createdAt DateTime @default(now())

  @@index([sessionId])
  @@index([createdAt])
}
